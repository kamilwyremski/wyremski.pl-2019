{
  "name": "wyremski.pl",
  "version": "1.0.8",
  "private": false,
  "homepage": "https://wyremski.pl",
  "dependencies": {
    "autoprefixer": "^9.8.6",
    "node-sass": "^4.14.1",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-ga": "^3.3.0",
    "react-google-recaptcha": "^2.1.0",
    "react-image-lightbox": "^5.1.1",
    "react-intl": "^5.10.6",
    "react-meta-tags": "^0.7.4",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^3.4.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "postbuild": "react-snap",
    "generate-sw": "sw-precache --root=build --config scripts/sw-precache-config.js && uglifyjs build/service-worker.js -o build/service-worker.js",
    "build-snap": "react-scripts build && react-snap && sw-precache --config=sw-precache-config.js"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "prop-types": "^15.7.2",
    "react-localize-redux": "^3.5.3",
    "react-snap": "^1.23.0",
    "typescript": "^3.9.7"
  },
  "reactSnap": {
    "cacheAjaxRequests": true,
    "skipThirdPartyRequests": true
  }
}
